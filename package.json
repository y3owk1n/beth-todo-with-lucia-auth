{
  "name": "beth-stack",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "script-to-dist": "bun run src/util/get-scripts-to-dist.ts",
    "tw:dev": "tailwindcss -i src/styles/index.css -o public/dist/main.css --watch",
    "dev": "concurrently \"bun run script-to-dist\" \"bun run tw:dev\" \"bun run --watch index.tsx\"",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:studio": "drizzle-kit studio --port 3010 --verbose"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/sanitize-html": "^2.9.0",
    "autoprefixer": "^10.4.15",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typed-html": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.6.1",
    "@elysiajs/html": "^0.6.4",
    "@elysiajs/static": "^0.6.0",
    "@fontsource-variable/inter": "^5.0.8",
    "@lucia-auth/adapter-postgresql": "^2.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sinclair/typebox": "^0.31.14",
    "@types/cookie": "^0.5.2",
    "cookie": "^0.5.0",
    "drizzle-orm": "^0.28.6",
    "elysia": "^0.6.20",
    "elysia-autoroutes": "^0.2.2",
    "elysia-helmet": "^1.0.1",
    "lucia": "^2.5.0",
    "postgres": "^3.3.5",
    "sanitize-html": "^2.11.0",
    "zod": "^3.22.2"
  }
}
